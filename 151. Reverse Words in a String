class Solution {
    public String reverseWords(String s) {
        String arr[]=s.trim().replaceAll(" +"," ").split(" ");
        s="";
        for(int i=0;i<arr.length;i++)
        {
            if(i==0)
            {
                s=arr[i]+s;
            }
            else
            {
                s=arr[i]+" "+s;
            }
            System.out.println(s);
        }
        return s;
    }
}
