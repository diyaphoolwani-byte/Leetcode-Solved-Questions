class Solution {
    public int[][] construct2DArray(int[] original, int m, int n) {
        int new_arr[][]=new int[m][n];
        int arr[][]=new int[0][0];
        int index=0;
        if(original.length!=m*n)
        {
            return arr;
        }
        for(int i=0;i<m;i++)
        {
            for(int j=0;j<n;j++)
             {
                new_arr[i][j]=original[index];
                index++;
             }
        }
        return new_arr;
    }
}
